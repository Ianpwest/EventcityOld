<!DOCTYPE html>
<html style="height:100%;">
<head>
    <meta charset="utf-8" />
    <title>@ViewBag.Title</title>
    <link href="@Url.Content("~/Content/StyleSheet1.css")" rel="stylesheet" type="text/css" />    
    <link href="@Url.Content("~/Content/CSS/jquery.qtip.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/CSS/jquery.qtip.min.css")" rel="stylesheet" type="text/css" />

    <script src="@Url.Content("~/Scripts/jquery-1.5.1.min.js")" type="text/javascript"></script>
