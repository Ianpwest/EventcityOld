<body  id="body" style="height:100%; width:1250px;">
    <script type="text/javascript">
        var width = screen.width;
        width = width - 10;
        document.getElementById("body").style.width = width + "px";
    </script>
        <table id="tableHeader" style="height:130px;width:100%">
            <tr>
		    <td id="divLogo" style="position:relative; float:left; height: 100%; width: 33%">
                
		    </td>
		    <td id="tdTitle" style="float:left; position:relative; width:33%; height:100%">
		    	<img src="../../Content/Evencity_Logo.jpg" alt="" height="100%" width="100%" />
		    </td>
		    <td style="float:right; width:33%; height:100%">
			    <div style="height:55%">
                <div id="logindisplay">
                    @Html.Partial("_LogOnPartial")
                </div>
			    </div>
			    <div style="height:40%">
			        <div>
                    <div id="menucontainer">
                        <ul id="menu">
                            <li>@Html.ActionLink("Home", "Index", "Home")</li>
                            <li>@Html.ActionLink("+Test Events", "CreateTestEvents", new {listNailVentToday = ViewBag.listNailVentToday, listNailVentWeek = ViewBag.listNailVentWeek, listNailVentMonth = ViewBag.listNailVentMonth, strState = "Tennessee" })</li>
                            <li>@Html.ActionLink("+Event", "Create", "Event")</li>
                        </ul>
                    </div>
			        </div>
			    </div>
		    </td>
            </tr>
	    </table>
        <table style="width:100%; height:70%; position:relative;">
            <tr>
                <td class="tdPad">                
                    <label id="lbl">This</label>                
                </td>

                <td>                
                    <div id="divRenderBody">
                    @RenderBody()
                    </div>
                </td>

                <td class="tdPad">
                @*@Html.DropDownList("States", (System.Web.Mvc.SelectList)ViewBag.StateList)*@
                </td>
            </tr>
        </table>

        <div id="divFooter">
            
        </div>
    
</body>
</html>

